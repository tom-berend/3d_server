{
    "name": "3d_server",
    "version": "0.0.1",
    "description": "Server for 3D course.  Incls discord bot.",
    "main": "dist/app.js",
    "scripts": {
        "start": "npm run serve",
        "build": "npm run build-sass && npm run build-ts",
        "oldbuild": "npm run build-sass && npm run build-ts && npm run lint && npm run copy-static-assets",
        "serve": "node dist/server.js",
        "watch-node": "nodemon dist/server.js",
        "watch": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run watch-node\"",
        "test": "jest --forceExit --verbose --detectOpenHandles",
        "watch-test": "npm run test -- --watchAll",
        "build-ts": "tsc",
        "watch-ts": "tsc -w",
        "build-sass": "node-sass src/server.scss views/server.css",
        "watch-sass": "node-sass -w src/server.scss views/server.css",
        "lint": "tslint --project '.' --fix",
        "copy-static-assets": "ts-node copyStaticAssets.ts",
        "debug": "npm run build && npm run watch-debug",
        "serve-debug": "nodemon --inspect dist/server.js",
        "watch-debug": "concurrently -k -p \"[{name}]\" -n \"Sass,TypeScript,Node\" -c \"yellow.bold,cyan.bold,green.bold\" \"npm run watch-sass\" \"npm run watch-ts\" \"npm run serve-debug\""
    },
    "author": "Tom Berend",
    "license": "MIT",
    "homepage": "https://communityreading.org",
    "dependencies": {
        "@types/better-sqlite3": "^5.4.0",
        "@types/body-parser": "^1.17.1",
        "@types/chalk": "^2.2.0",
        "@types/cors": "^2.8.6",
        "@types/debug": "^4.1.5",
        "@types/express": "^4.17.2",
        "@types/express-session": "^1.15.16",
        "@types/node": "^12.12.17",
        "@types/sqlite3": "^3.1.5",
        "@types/ws": "^6.0.4",
        "body-parser": "^1.19.0",
        "chalk": "^2.4.2",
        "cors": "^2.8.5",
        "debug": "^4.1.1",
        "discord.js": "github:discordjs/discord.js",
        "ejs": "^2.6.2",
        "express": "^4.17.1",
        "express-session": "^1.17.0",
        "memorystore": "^1.6.1",
        "sqlite3": "^4.1.1",
        "ws": "^7.4.6"
    },
    "devDependencies": {
        "@types/jest": "^24.0.18",
        "concurrently": "^5.0.0",
        "jest": "^24.9.0",
        "jest-preset-typescript": "^1.2.0",
        "node-sass": "^4.13.0",
        "nodemon": "^2.0.1",
        "ts-jest": "^24.2.0",
        "ts-loader": "^6.0.1",
        "tslint": "^5.11.0",
        "typescript": "^3.6.4"
    },
    "comments": {
        "01": "x",
        "02": "x",
        "03": "x",
        "04": "x",
        "05": "x"
    }
}
